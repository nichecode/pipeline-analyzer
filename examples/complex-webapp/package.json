{
  "name": "complex-webapp-example",
  "version": "2.1.4",
  "description": "Complex web application with comprehensive CI/CD pipeline",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack serve --mode development",
    "start:prod": "node server.js",
    "start:test": "NODE_ENV=test node server.js",
    "start:e2e": "NODE_ENV=e2e node server.js",
    "build": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "build:analyze": "webpack-bundle-analyzer dist/stats.json",
    "test:unit": "jest",
    "test:integration": "mocha test/integration/**/*.test.js",
    "test:e2e": "cypress run",
    "test:load": "artillery run test/load/load-test.yml",
    "test:smoke": "mocha test/smoke/**/*.test.js",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:css": "stylelint src/**/*.css src/**/*.scss",
    "format:check": "prettier --check src/",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "express": "^4.18.2",
    "axios": "^1.4.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-bundle-analyzer": "^4.9.0",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "mocha": "^10.2.0",
    "cypress": "^12.17.1",
    "artillery": "^2.0.0",
    "eslint": "^8.44.0",
    "stylelint": "^15.10.1",
    "prettier": "^2.8.8",
    "codecov": "^3.8.3"
  }
}